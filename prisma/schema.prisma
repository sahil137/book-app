
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}


model Book {
  id Int @id @default(autoincrement())
  name String @unique
  description String
  authors String[]
  readTime Int
  coverImage String @db.Text
  pdf String @db.Text
  rating Int 
  createdAt  DateTime   @default(now())
}